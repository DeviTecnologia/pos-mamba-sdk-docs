{
  "name": "@mambasdk/docs",
  "description": "Docs and examples for Mamba Web",
  "version": "0.0.1",
  "author": "Stone Payments - Mamba Team",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "cross-env NODE_ENV=production sapper build",
    "export": "sapper export",
    "start": "node build",
    "debug": "DEBUG=express:*, sapper dev",
    "cy:run": "cypress run",
    "deploy": "now",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "lint": "npm run lint:js",
    "lint:js": "eslint \"*.js\" \"{tools,packages,docs-sapper}/**/*.{js,html,svelte}\"",
    "format": "npm run format:css; npm run format:js",
    "format:js": "prettier --loglevel silent --write \"*.js\" \"{tools,packages,docs-sapper}/**/*.js\" && eslint --fix \"*.js\" \"{tools,packages,docs-sapper}/**/*.{js,html,svelte}\"",
    "format:css": "prettier --loglevel silent --write \"packages/**/*.{css,pcss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,svelte}": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{pcss,css}": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@mambasdk/app": "latest",
    "@mambasdk/appbar": "latest",
    "@mambasdk/barcode": "latest",
    "@mambasdk/button": "latest",
    "@mambasdk/collection": "latest",
    "@mambasdk/dialog": "latest",
    "@mambasdk/icon": "^0.0.2",
    "@mambasdk/input": "latest",
    "@mambasdk/pos": "latest",
    "@mambasdk/printable": "latest",
    "@mambasdk/progress": "latest",
    "@mambasdk/qrcode": "latest",
    "@mambasdk/range": "latest",
    "@mambasdk/sprite": "latest",
    "@mambasdk/switch": "latest",
    "cheerio": "^1.0.0-rc.2",
    "codemirror": "^5.36.0",
    "compression": "^1.7.2",
    "do-not-zip": "^0.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "globby": "^8.0.1",
    "golden-fleece": "^1.0.9",
    "locate-character": "^2.0.5",
    "lodash": "^4.17.10",
    "marked": "^0.4.0",
    "node-fetch": "^2.2.0",
    "prismjs": "^1.14.0",
    "rollup": "^0.63.4",
    "serve-static": "^1.13.2",
    "sourcemap-codec": "^1.4.1",
    "svelte-extras": "^2.0.2",
    "svelte-routing": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-prismjs": "^1.0.2",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.0",
    "mini-html-webpack-plugin": "^0.2.3",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.2",
    "postcss-advanced-variables": "^2.3.3",
    "postcss-atroot": "^0.1.3",
    "postcss-cli": "^6.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-extend-rule": "^2.0.0",
    "postcss-html": "^0.32.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^5.1.0",
    "postcss-property-lookup": "^2.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-syntax": "^0.32.0",
    "postcss-url": "^7.3.2",
    "prettier": "^1.14.0",
    "prismjs": "^1.14.0",
    "quickenv": "^0.0.5",
    "sapper": "^0.15.5",
    "style-loader": "^0.21.0",
    "stylelint": "^9.4.0",
    "svelte": "^2.8.0",
    "svelte-loader": "^2.9.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3",
    "workerize-loader": "^1.0.3"
  }
}
